#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/jest@27.5.1/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules:/Users/mac/Upwork/parachute-interview/node_modules:/Users/mac/Upwork/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/jest/bin/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/jest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/jest@27.5.1/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules:/Users/mac/Upwork/parachute-interview/node_modules:/Users/mac/Upwork/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/jest/bin/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/jest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
