#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/fixtures/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/import-local@3.0.2/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules:/Users/mac/Upwork/parachute-interview/node_modules:/Users/mac/Upwork/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/fixtures/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/import-local@3.0.2/node_modules/import-local/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/import-local@3.0.2/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules/.pnpm/node_modules:/Users/mac/Upwork/parachute-interview/typescript/node_modules:/Users/mac/Upwork/parachute-interview/node_modules:/Users/mac/Upwork/node_modules:/Users/mac/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../fixtures/cli.js" "$@"
else
  exec node  "$basedir/../../fixtures/cli.js" "$@"
fi
